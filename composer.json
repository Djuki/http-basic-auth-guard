{
    "name": "arubacao/http-basic-auth-guard",
    "description": "HTTP Basic Auth Guard for Laravel and Lumen",
    "keywords": ["laravel http basic auth guard", "lumen http basic auth guard", "http basic auth guard", "http basic authentication"],
    "homepage": "https://github.com/arubacao/http-basic-auth-guard",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Lass",
            "email": "arubacao@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "graham-campbell/testbench": "^3.2",
        "laravel/laravel" : "5.2.*",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Arubacao\\BasicAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arubacao\\Tests\\BasicAuth\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
